{
  "name": "@vbots/cmenu",
  "description": "Menu system for the library vk-io",
  "version": "0.0.3",
  "main": "lib/index",
  "author": "xTCry (Vladislav Kh)",
  "license": "MIT",
  "scripts": {
    "pubfix": "npm version patch && yarn run publish",
    "publish": "yarn run prepare && npm publish --access public",
    "build_": "tsc && yarn run test && node ./dist/index.js",
    "prepare": "yarn run rollup:build && yarn run test",
    "build": "yarn run rollup:build",
    "watch": "yarn run rollup:build -w",
    "rollup:build": "rollup -c rollup.config.ts",
    "test": "yarn run test:jest",
    "test_": "yarn run test:jest && yarn run test:eslint",
    "test:jest": "jest --config jest.config.json --no-cache",
    "test:eslint": "eslint --ext .ts,.js --ignore-path .gitignore src/**/*.{ts,js}"
  },
  "dependencies": {
    "vk-io": "^4.0.0-rc.25"
  },
  "devDependencies": {
    "@types/jest": "^24.0.21",
    "@types/node": "^12.12.3",
    "@types/rollup-plugin-json": "^3.0.2",
    "@types/tough-cookie": "^2.3.5",
    "@types/ws": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "@vk-io/session": "^0.0.0-alpha.2",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0",
    "rollup": "^1.26.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.24.3",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "repository": "git@github.com:VBots/CMenu.git",
  "bugs": {
    "url": "https://github.com/VBots/CMenu/issues"
  },
  "homepage": "https://github.com/VBots/CMenu#readme"
}
